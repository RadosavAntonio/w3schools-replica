-
    var topNav = [
        {label: 'Home', link: '/'},
        {label: 'HTML', link: '/html'},
        {label: 'CSS', link: '/css'},
        {label: 'JavaScript', link: '/javascript'},
    ];


header
    //- div=landingPage

    //- a.current(href=`/`) Home
    //- a(href=`/html`) HTML
    //- a(href=`/css`) CSS
    //- a(href=`/javascript`) JavaScript

    each navigation in topNav
        if navigation.label == landingPage
            a.current(href=navigation.link)=navigation.label
        else
            a(href=navigation.link)=navigation.label
